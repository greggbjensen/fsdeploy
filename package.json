{
  "name": "fsdeploy",
  "displayName": "fsdeploy",
  "description": "Deploy workspace files after saving to a specified destination",
  "version": "0.1.0",
  "publisher": "mightycoco",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "keywords": [
    "sync",
    "deploy",
    "copy"
  ],
  "repository": {
		"type": "git",
		"url": "https://github.com/mightycoco/fsdeploy.git"
	},
  "galleryBanner": {
    "color": "#9ebeff",
    "theme": "dark"
  },
  "main": "./out/src/fsdeploy",
  "contributes": {
    "commands": [
      {
        "command": "fsdeploy.deployFile",
        "title": "Deploy File",
        "category": "fsdeploy"
      },
      {
        "command": "fsdeploy.deployWorkspace",
        "title": "Deploy Workspace",
        "category": "fsdeploy"
      },
      {
        "command": "fsdeploy.menuFunctions",
        "title": "Show fsdeploy functions",
        "category": "fsdeploy"
      }
    ]
  },
  "icon": "logo.svg",
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "fs-extra": "^0.30.0"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  }
}
